^.*\.Rproj$
^\.Rproj\.user$
^\.git$
^\.github$
^\.gitignore$
^\.gitattributes$

# Ignore the obsolete libtool file with a non-portable name
^src/hdf5-1.14.6/m4/lt~obsolete\.m4$

# Ignore the bundled citation file
^src/hdf5-1.14.6/CITATION\.cff$

# Ignore Java contents (keeping java/Makefile.in and java/test/Makefile.in)
^src/hdf5-1.14.6/java/.*\.(java|jar|class|h|c|cpp|sh|bat|txt|docbook|pdf|png|css|html|xml|xsl|ipynb)$
# Ignore Java test contents (keeping java/test/Makefile.in)
^src/hdf5-1.14.6/java/test/.*\.(java|h|c|sh|h5|txt)$

# Ignore Examples directory (keeping HDF5Examples/Makefile.in)
^src/hdf5-1.14.6/HDF5Examples/.*\.(c|h|f90|cpp|h5|txt|out|out\.txt|ddl|jpg)$

# --- Ignore contents of other directories BUT keep *.in files ---

# Ignore C++ contents (keeping c++/Makefile.in)
^src/hdf5-1.14.6/c++/examples
^src/hdf5-1.14.6/c++/src/.*\.(cpp|h)$
^src/hdf5-1.14.6/c++/test/.*\.(cpp|h|sh|ddl|txt)$

# Ignore Fortran contents (keeping fortran/Makefile.in and fortran/src/h5fc.in)
^src/hdf5-1.14.6/fortran/examples
^src/hdf5-1.14.6/fortran/src/.*\.(f90|inc|h)$
^src/hdf5-1.14.6/fortran/test/.*\.(f90|h5|sh|txt|dat)$
^src/hdf5-1.14.6/fortran/testpar/.*\.(f90|h5|sh|txt|dat)$

# Ignore HL C++ contents (keeping hl/c++/Makefile.in)
^src/hdf5-1.14.6/hl/c++/examples
^src/hdf5-1.14.6/hl/c++/src/.*\.(cpp|h)$
^src/hdf5-1.14.6/hl/c++/test/.*\.(cpp|h5|sh)$

# Ignore HL Fortran contents (keeping hl/fortran/Makefile.in)
^src/hdf5-1.14.6/hl/fortran/examples
^src/hdf5-1.14.6/hl/fortran/src/.*\.(f90|inc)$
^src/hdf5-1.14.6/hl/fortran/test/.*\.(f90|h5|sh)$

# Ignore HL examples contents (keeping hl/examples/Makefile.in)
^src/hdf5-1.14.6/hl/examples/.*\.(c|h|f90|h5|py|ipynb)$

# Ignore HL test contents (keeping hl/test/Makefile.in)
^src/hdf5-1.14.6/hl/test/.*\.(c|h|sh|h5|txt|dat|gif)$

# Ignore HL tool contents (keeping hl/tools/Makefile.in and sub-Makefiles)
