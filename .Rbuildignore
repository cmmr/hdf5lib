^.*\.Rproj$
^\.Rproj\.user$
^\.git$
^\.github$
^\.gitignore$
^\.gitattributes$

# Ignore the obsolete libtool file with a non-portable name
^src/hdf5-1.14.6/m4/lt~obsolete\.m4$

# Ignore the bundled citation file
^src/hdf5-1.14.6/CITATION\.cff$

# Ignore Java components
^src/hdf5-1.14.6/java

# Ignore Examples directory
^src/hdf5-1.14.6/HDF5Examples

# --- Ignore contents of other directories BUT keep Makefiles and .in files ---

# Ignore C++ contents (keeping c++/Makefile.in)
^src/hdf5-1.14.6/c++/examples
^src/hdf5-1.14.6/c++/src/.*\.(cpp|h)$
^src/hdf5-1.14.6/c++/test

# Ignore Fortran contents (keeping fortran/Makefile.in and fortran/src/h5fc.in)
^src/hdf5-1.14.6/fortran/examples
^src/hdf5-1.14.6/fortran/src/.*\.(f90|inc|h)$
^src/hdf5-1.14.6/fortran/test
^src/hdf5-1.14.6/fortran/testpar

# Ignore HL C++ contents (keeping hl/c++/Makefile.in)
^src/hdf5-1.14.6/hl/c++/examples
^src/hdf5-1.14.6/hl/c++/src/.*\.(cpp|h)$
^src/hdf5-1.14.6/hl/c++/test

# Ignore HL Fortran contents (keeping hl/fortran/Makefile.in)
^src/hdf5-1.14.6/hl/fortran/examples
^src/hdf5-1.14.6/hl/fortran/src/.*\.(f90|inc)$
^src/hdf5-1.14.6/hl/fortran/test

# Ignore HL examples (contains Makefile.in, but --disable-tests should cover)
^src/hdf5-1.14.6/hl/examples

# Ignore HL tests (keeping hl/test/Makefile.in)
^src/hdf5-1.14.6/hl/test/.*\.(c|h|sh)$

# Ignore HL tool contents (keeping hl/tools/Makefile.in and sub-Makefiles)
^src/hdf5-1.14.6/hl/tools/gif2h5/.*\.c$
^src/hdf5-1.14.6/hl/tools/h5watch/.*\.c$

# Ignore Test contents (keeping test/Makefile.in)
^src/hdf5-1.14.6/test/.*\.(c|h|h5|txt|inc|ddl|dat|sh)$
^src/hdf5-1.14.6/test/test H5T VLEN\.h5$
^src/hdf5-1.14.6/test/vds_data

# Ignore Test Parallel contents (keeping testpar/Makefile.in)
^src/hdf5-1.14.6/testpar/.*\.(c|h5|txt)$
^src/hdf5-1.14.6/testpar/cache
^src/hdf5-1.14.6/testpar/tcio

# Ignore Tools contents (keeping Makefiles and *.in files in tools/ and subdirs)
^src/hdf5-1.14.6/tools/lib/.*\.c$
^src/hdf5-1.14.6/tools/libtest/.*\.c$
^src/hdf5-1.14.6/tools/src/.*\.(c|h)$
^src/hdf5-1.14.6/tools/src/.*/.*\.(c|h)$ # Ignore C/H files in tools/src subdirs
^src/hdf5-1.14.6/tools/test/.*\.(c|h|sh)$
^src/hdf5-1.14.6/tools/test/.*/.*\.(c|h|sh)$ # Ignore C/H/SH files in tools/test subdirs
^src/hdf5-1.14.6/tools/test/h5dump/testfiles
^src/hdf5-1.14.6/tools/test/h5repack/expected

# Ignore Utils contents (keeping Makefiles and *.in files in utils/ and subdirs)
^src/hdf5-1.14.6/utils/mirror_vfd/.*\.c$
^src/hdf5-1.14.6/utils/test/.*\.c$
^src/hdf5-1.14.6/utils/tools/.*\.c$
^src/hdf5-1.14.6/utils/tools/h5dwalk/.*\.(c|h)$
^src/hdf5-1.14.6/utils/tools/test/.*\.c$
^src/hdf5-1.14.6/utils/tools/test/h5dwalk/.*\.(c|h|sh)$
^src/hdf5-1.14.6/utils/tools/test/h5dwalk/testfiles

# --- Ignore zlib files we don't need ---
# (Keeping zlib-1.3.1/Makefile.in, zconf.h.in, configure, win32/Makefile.gcc)
^src/zlib-1.3.1/contrib
^src/zlib-1.3.1/examples
^src/zlib-1.3.1/nintendods
^src/zlib-1.3.1/test
^src/zlib-1.3.1/amiga
^src/zlib-1.3.1/msdos
^src/zlib-1.3.1/old
^src/zlib-1.3.1/os400
^src/zlib-1.3.1/qnx
^src/zlib-1.3.1/vms
^src/zlib-1.3.1/win32/.*\.(c|def|rc)$
^src/zlib-1.3.1/CMakeLists\.txt$
^src/zlib-1.3.1/zconf\.h\.cmakein$
^src/zlib-1.3.1/zlib\.pc\.cmakein$
